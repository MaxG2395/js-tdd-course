{
  "name": "js-com-tdd",
  "version": "1.0.0",
  "description": "Biblioteca do curso de JS com TDD na pratica.",
  "main": "app.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "build": "./node_modules/.bin/webpack --colors --progress --mode development",
    "watch": "npm run build -- --watch",
    "server": "./node_modules/.bin/webpack-dev-server --mode development --inline --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaxG2395/js-tdd-course.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Max Ganzer <max_ganzer@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MaxG2395/js-tdd-course/issues"
  },
  "homepage": "https://github.com/MaxG2395/js-tdd-course#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.1",
    "mocha": "^5.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "ramda": "^0.26.1"
  }
}
